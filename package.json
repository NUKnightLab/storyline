{
  "name": "storyline",
  "version": "1.0.3",
  "description": "Storyline is a tool for journalists to provide narrative context to readable line charts in a form which is optimized for the mobile user experience.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NUKnightLab/storyline.git"
  },
  "author": "Northwestern University Knight Lab",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NUKnightLab/storyline/issues"
  },
  "homepage": "https://storyline.knightlab.com",
  "devDependencies": {
    "add-banner": "github:jonschlinkert/add-banner#b2a824c466413677264364042bb4c30df0de2ebe",
    "adm-zip": "~> 0.4.11",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "cssmin": "^0.4.3",
    "csv-parse": "^1.1.9",
    "d3-axis": "^1.0.5",
    "d3-scale": "^1.0.4",
    "d3-selection": "^1.0.4",
    "d3-time-format": "^2.0.5",
    "es6-promise": "^4.0.5",
    "exit-hook": "^1.1.1",
    "fs-extra": "^0.30.0",
    "glob": "^7.1.2",
    "globby": "^5.0.0",
    "hammerjs": "^2.0.8",
    "lodash.throttle": "^4.1.1",
    "mocha": "^3.3.0",
    "moment": "^2.17.1",
    "mustache": "^2.3.0",
    "ncp": "^2.0.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "prompt": "^1.0.0",
    "pubsub-js": "^1.5.4",
    "rimraf": "^2.5.3",
    "shelljs": "^0.7.0",
    "simple-git": "^1.67.0",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "stream-transform": "^0.1.1",
    "tiny-lr": "^0.2.1",
    "universal-analytics": "^0.4.23",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^5.1.2",
    "write-banner": "^0.1.3",
    "xhr-stream": "^1.1.0"
  },
  "scripts": {
    "start": "npm-run-all -p webpack:watch watch",
    "webpack:watch": "webpack --config webpack.dev.js  --watch",
    "prestart": "npm run clean && webpack --config webpack.dev.js && npm run compile",
    "dist": "webpack --config webpack.prod.js -p && npm run compile && npm run sass && node tasks/minify-css.js",
    "clean": "rimraf dist/",
    "watch": "node tasks/watch.js",
    "server": "http-server ./dist -c-1",
    "compile": "npm run copy && npm run sass",
    "copy": "npx ncp src/index.html dist/index.html && mkdir -p dist/embed && npx ncp src/embed/index.html dist/embed/index.html --stopOnErr && ncp src/assets dist/assets --stopOnErr",
    "sass": "npx node-sass -o dist/css -q --output-style compact src/scss/",
    "test": "mocha --compilers js:babel-core/register --require babel-polyfill './test/*-test.js'",
    "stage": "npm run clean && npm run dist && node tasks/stage.js",
    "stage_latest": "npm run clean && npm run dist && node tasks/stage.js latest",
    "stage_dev": "npm run clean && npm run dist && node tasks/stage.js dev"
  },
