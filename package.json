    "chokidar": "^1.6.0",
    "cssmin": "^0.4.3",
    "csv-parse": "^1.1.9",
    "d3-axis": "^1.0.5",
    "d3-scale": "^1.0.4",
    "d3-selection": "^1.0.4",
    "d3-time-format": "^2.0.5",
    "es6-promise": "^4.0.5",
    "exit-hook": "^1.1.1",
    "fs-extra": "^0.30.0",
    "glob": "^7.1.2",
    "globby": "^5.0.0",
    "hammerjs": "^2.0.8",
    "json-loader": "^0.5.4",
    "lodash.throttle": "^4.1.1",
    "mocha": "^3.3.0",
    "moment": "^2.17.1",
    "mustache": "^2.3.0",
    "prompt": "^1.0.0",
    "pubsub-js": "^1.5.4",
    "rimraf": "^2.5.3",
    "shelljs": "^0.7.0",
    "simple-git": "^1.67.0",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
