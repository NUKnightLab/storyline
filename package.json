  },
  "homepage": "https://storyline.knightlab.com",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "http-server": "^0.9.0",
    "ncp": "^2.0.0",
    "node-sass": "^3.8.0",
    "parallelshell": "^2.0.0",
    "universal-analytics": "^0.4.14",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "add-banner": "github:jonschlinkert/add-banner#b2a824c466413677264364042bb4c30df0de2ebe",
